{
  "name": "betahealth",
  "version": "2.9.0",
  "private": true,
  "scripts": {
    "heroku-postbuild": "gulp build",
    "start": "node app.js",
    "develop": "env $(cat .env) gulp watch",
    "lint": "gulp lint-js && gulp lint-css",
    "test": "npm run test:unit",
    "test:unit": "NODE_ENV=test LOG_LEVEL=silent istanbul test _mocha -- ./test/unit --opts ./test/unit/mocha.opts -R spec",
    "test:acceptance": "LOG_LEVEL=info wdio test/acceptance/wdio.conf.js",
    "test:acceptance-headless": "LOG_LEVEL=info wdio test/acceptance/wdio.headless.conf.js",
    "test:acceptance-cloud": "LOG_LEVEL=info wdio test/acceptance/wdio.cloud.conf.js",
    "test:acceptance-ci": "bash ./bin/run-acceptance-tests.sh",
    "coverage": "npm_config_coverage=true npm run test:unit",
    "coverage:send": "codeclimate-test-reporter < ./coverage/lcov.info",
    "coverage:check": "istanbul check-coverage --config .istanbul.yml",
    "docs:serve": "node $NODE_DEBUG_OPTION docs/serve",
    "docs:build": "node docs/build"
  },
  "engines": {
    "node": ">=6.0.0 <7.0.0"
  },
  "dependencies": {
    "JSON2": "~0.1.0",
    "applicationinsights": "~0.17.0",
    "async": "~2.1.4",
    "babel-core": "~6.18.2",
    "babel-loader": "~6.2.7",
    "babel-preset-es2015": "~6.18.0",
    "body-parser": "~1.15.2",
    "change-case": "~3.0.0",
    "churchill": "~0.2.0",
    "compression": "~1.6.2",
    "cookie-parser": "~1.4.3",
    "csurf": "~1.9.0",
    "del": "~2.2.2",
    "express": "~4.14.0",
    "express-sslify": "~1.1.0",
    "express-validator": "~2.21.0",
    "gulp": "~3.9.1",
    "gulp-autoprefixer": "~3.1.1",
    "gulp-if": "~2.0.2",
    "gulp-ignore": "~2.0.2",
    "gulp-imagemin": "~3.1.1",
    "gulp-preprocess": "~2.0.0",
    "gulp-print": "~2.0.1",
    "gulp-require-tasks": "~1.0.5",
    "gulp-rev-all": "~0.9.7",
    "gulp-sass": "~2.3.2",
    "gulp-sequence": "~0.4.6",
    "gulp-sourcemaps": "~2.2.0",
    "gulp-util": "~3.0.7",
    "helmet": "~3.1.0",
    "html5shiv": "~3.7.3",
    "http-proxy-middleware": "~0.17.2",
    "jquery": "~1.12.4",
    "lodash": "~4.17.2",
    "markdown-it": "~8.1.0",
    "markdown-it-abbr": "~1.0.4",
    "markdown-it-attrs": "~0.8.0",
    "markdown-it-container": "~2.0.0",
    "markdown-it-named-headers": "0.0.4",
    "method-override": "~2.3.6",
    "moment": "~2.16.0",
    "moment-timezone": "~0.5.9",
    "normalize.css": "~5.0.0",
    "nunjucks": "~3.0.0",
    "nunjucks-markdown": "~2.0.1",
    "parseurl": "~1.3.1",
    "picturefill": "~3.0.2",
    "pre-commit": "~1.1.3",
    "request": "~2.78.0",
    "request-ip": "~1.2.3",
    "serve-favicon": "~2.3.0",
    "slashify": "~1.0.0",
    "tsscmp": "~1.0.5",
    "webpack": "1.13.2",
    "winston": "~2.3.0",
    "winston-azure-application-insights": "nhsuk/winston-azure-application-insights#843afd2b8a631eae3537cfee0e146b761ba0a939"
  },
  "devDependencies": {
    "browser-sync": "~2.18.1",
